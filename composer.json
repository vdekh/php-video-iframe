{
    "name": "vdekh/php-video-iframe",
    "description": "Converts video host links to an iframe tag in PHP.",
    "homepage": "https://github.com/vdekh/php-video-iframe",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Dekh",
            "email": "vicdekh@gmail.com",
            "role": "Development"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "vdekh\\videoiframe\\": "src/videoiframe/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "serve": "php -S 0.0.0.0:8080 -t public",
        "test": "phpunit --colors=always"
    }
}
